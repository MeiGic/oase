<?xml version='1.0' encoding='utf-8'?>
<widget id="io.meigic.oasedeliveryapp" version="0.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <feature name="ScanditSDK">
        <param name="android-package" value="com.mirasense.scanditsdk.plugin.ScanditSDK" />
    </feature>
    <name>OASE Delivery</name>
    <description>
        A Delivery System using in OASE.
    </description>
    <author email="officialmeigic@gmail.com" href="http://github.com/MeiGic/oase/">
        MeiGic 2014
    </author>
    <preference name="Fullscreen" value="true" />
    <preference name="Orientation" value="portrait" />
    <icon height="256" src="www/img/icon.png" width="256" />
    <content src="index.html" />
    <access origin="*" />
</widget>
